library("httr")
library("readxl")
library("leaflet")
library("dplyr")
library("ggplot2")
library("htmltools")
GET("https://query.data.world/s/rjngr5uzhv4ihxvhak3bvny66uoj47", write_disk(tf1 <- tempfile(fileext = ".xlsx")))
df1 <- read_excel(tf1)
data<-df1
data_actuals<-data%>%slice_sample(prop=0.025)
data_actuals%>%mutate(Power=ifelse())
# Characters to factors
data_actuals[sapply(data_actuals, is.character)] <- lapply(data_actuals[sapply(data_actuals, is.character)],
as.factor)
data_distribution<-data_actuals%>%filter(PV=="Distribution PV")
data_utility<-data_actuals%>%filter(PV=="Utility Scale PV")
palette<-colorFactor(palette=c("blue","red"),levels=c("Distribution PV","Utility Scale PV"))  #esto debe estar igual al dato
leaflet(options=leafletOptions(dragging=FALSE,minZoom=7,maxZoom=18))%>%
addTiles(group="OSM")%>%
addProviderTiles("CartoDB",group="Carto")%>%
addProviderTiles("Esri",group="Esri")%>%
addCircleMarkers(data=data_distribution,
color=~palette(PV),
lng=~Longitude,
lat=~Latitude,
radius=~`Power(MW)`/70,
label=~Capacity,
group="Distribution PV")%>%
addCircleMarkers(data=data_utility,
color=~palette(PV),
lng=~Longitude,
lat=~Latitude,
radius=~`Power(MW)`/70,
label=~Capacity,
group="Utility Scale PV")%>%
addLayersControl(baseGroups=c("OSM","Carto","Esri"),overlayGroups = c("Distribution PV","Utility Scale PV"))%>%
addLegend(pal=palette,values = c("Distribution PV", "Utility Scale PV"),
opacity = 0.5, title = "PV", position = "topright")
